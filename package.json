  "bugs": {
    "url": "https://github.com/RosyTucker/CodeNightFrontend/issues"
  },
  "homepage": "https://github.com/RosyTucker/CodeNightFrontend#readme",
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.9.0",
    "chai": "3.5.0",
    "enzyme": "2.3.0",
    "eslint": "2.10.2",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "1.8.0",
